  <procedure xsi:type="apex:constantProcedureType">
    <parameters>
      <presentations>2</presentations>
      <skip>0</skip>
      <order>sequential</order>
      <choices>2</choices>
    </parameters>
    <trials>
      <trial id="trial100">
        <screen id="screen1"/>
        <stimulus id="stimulus_110"/>
        <standard id="stimulus_100"/>
      </trial>
      
