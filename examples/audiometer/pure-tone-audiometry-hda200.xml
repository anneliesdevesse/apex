  <procedure xsi:type="apex:trainingProcedureType">
    <parameters>
      <presentations>10000</presentations>
      <skip>0</skip>
      <order>sequential</order>
    </parameters>
    <trials>
      <plugintrials>
        <script source="inline">
          function getTrials() {
            misc.checkParams(Array("intensities", "frequencies"));
            var result = "";
            var intensities = params["intensities"].split(":");
            var frequencies = params["frequencies"].split(",");
            for (var frequency in frequencies) {
              for (var intensity = parseInt(intensities[0]); intensity &lt;= parseInt(intensities[2]); intensity += parseInt(intensities[1])) {
                var absintensity = Math.abs(intensity).toString();
                var sign = intensity &lt; 0 ? "m" : intensity == 0 ? "n" : "p";
                var id = frequencies[frequency] + sign + absintensity;
                result += xml.tag("trial", {"id": "trial" + id},
                  xml.tag("answer", {}, "button" + id),
                  xml.tag("screen", {"id": "screen1"}),
                  xml.tag("stimulus", {"id": "stimulus" + id}));
              }
            }
            return result;
          }
        </script>
        <parameter name="intensities">-10:5:70</parameter>
        <parameter name="frequencies">125,250,500,750,1000,1500,2000,3000,4000,6000,8000</parameter>
      </plugintrials>
    </trials>
  </procedure>

  <corrector xsi:type="apex:isequal"/>

  <screens>
    <pluginscreens>
      <script source="inline">
        function getScreens() {
          misc.checkParams(Array("intensities", "frequencies"));
          var result = "";
          var buttons = "";
          var labels1 = "", labels2 = "";
          var def = xml.tag("default_answer_element", {}, "buttongroup1");
          var intensities = params["intensities"].split(":");
          var frequencies = params["frequencies"].split(",");
          var intensitycount = 0;
          for (var frequency in frequencies) {
            intensitycount = 0;
            for (var intensity = parseInt(intensities[0]), intensityindex = 1; intensity &lt;= parseInt(intensities[2]); intensity += parseInt(intensities[1]), ++intensityindex) {
              ++intensitycount;
              var absintensity = Math.abs(intensity).toString();
              var sign = intensity &lt; 0 ? "m" : intensity == 0 ? "n" : "p";
              var id = frequencies[frequency] + sign + absintensity;
              result += xml.tag("button", {"id": "button" + id, "x": parseInt(frequency) + 1, "y": intensityindex + 1},
                xml.tag("style", {}, parseInt(frequency) % 2 == 0 ?
                  "QPushButton {background-color: darkgray}" : "QPushButton {background-color: lightgray}"),
                  xml.tag("bgcolor", {}, "darkblue"),
                  xml.tag("text", {}, intensity));
              buttons += xml.tag("button", {"id": "button" + id});
            }
          }
          for (var frequency in frequencies) {
            labels1 += xml.tag("label", {"id": "freq1" + frequency, "row": 1, "col": parseInt(frequency) + 1},
              xml.tag("bgcolor", {}, parseInt(frequency) % 2 == 0 ? "darkgrey" : "lightgrey"),
              xml.tag("text", {}, frequencies[frequency] + "Hz")),
            labels2 += xml.tag("label", {"id": "freq2" + frequency, "row": intensitycount + 2, "col": parseInt(frequency) + 1},
              xml.tag("bgcolor", {}, parseInt(frequency) % 2 == 0 ? "darkgrey" : "lightgrey"),
              xml.tag("text", {}, frequencies[frequency] + "Hz")),
          }
          buttons = xml.tag("buttongroup", {"id": "buttongroup1"}, buttons);
          result = xml.tag("screen", {"id": "screen1"},
            xml.tag("gridLayout", {"id": "grid1", "height": intensitycount + 2, "width": frequencies.length},
              labels1,
              labels2,
              result),
            buttons,
            def);
          return result;
        }

      </script>
      <parameter name="intensities">-10:5:70</parameter>
      <parameter name="frequencies">125,250,500,750,1000,1500,2000,3000,4000,6000,8000</parameter>
    </pluginscreens>
  </screens>

