<?xml version='1.0' encoding='UTF-8'?>
<apex:apex xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://med.kuleuven.be/exporl/apex/3.0.3/experiment https://gilbert.med.kuleuven.be/apex/schemas/3.0.3/experiment.xsd"
 xmlns:apex="http://med.kuleuven.be/exporl/apex/3.0.3/experiment"
 version="1">
  <!-- 
Try to add wav filter to L34Device
    -->
  <procedure xsi:type="apex:trainingProcedureType">
    <parameters>
      <presentations>100</presentations>
      <skip>0</skip>
      <order>sequential</order>
    </parameters>
    <trials>
      <trial id="trial1">
        <answer>button1</answer>
        <screen id="screen1"/>
        <stimulus id="stimulus1"/>
      </trial>
    </trials>
  </procedure>
  <corrector xsi:type="apex:isequal"/>
  <screens>
    <screen id="screen1">
        <gridLayout height="2" width="3" id="main_layout">
          
          <button x="1" y="1" id="button1">
            <text>mapping.aseq</text>
          </button>
          
          
        </gridLayout>
        <buttongroup id="buttongroup1">
          <button id="button1"/>
        </buttongroup>
        <default_answer_element>buttongroup1</default_answer_element>
        
      </screen>
  </screens>
  <datablocks>
    <uri_prefix>../stimuli/</uri_prefix>
    <datablock id="rawdata1">
      <device>l34device</device>
      <uri>mapping.aseq</uri>
    </datablock>
  </datablocks>
  <devices>
    <device id="l34device" xsi:type="apex:L34DeviceType">
      <device_id>0</device_id>
      <implant>cic3</implant>
      <trigger>none</trigger>
      <defaultmap>
        <inline>
          <number_electrodes>22</number_electrodes>
          <mode>MP1+2</mode>
          <pulsewidth>25</pulsewidth>
          <pulsegap>25</pulsegap>
          <period>150</period>
          <channel number="1" electrode="22" threshold="2" comfort="254"/>
          <channel number="2" electrode="21" threshold="3" comfort="253"/>
          <channel number="3" electrode="20" threshold="4" comfort="252"/>
          <channel number="4" electrode="19" threshold="5" comfort="251"/>
          <channel number="5" electrode="18" threshold="6" comfort="250"/>
          <channel number="6" electrode="17" threshold="7" comfort="249"/>
          <channel number="7" electrode="16" threshold="8" comfort="248"/>
          <channel number="8" electrode="15" threshold="9" comfort="247"/>
          <channel number="9" electrode="14" threshold="10" comfort="246"/>
          <channel number="10" electrode="13" threshold="11" comfort="245"/>
          <channel number="11" electrode="12" threshold="12" comfort="244"/>
          <channel number="12" electrode="11" threshold="13" comfort="243"/>
          <channel number="13" electrode="10" threshold="14" comfort="242"/>
          <channel number="14" electrode="9" threshold="15" comfort="241"/>
          <channel number="15" electrode="8" threshold="16" comfort="240"/>
          <channel number="16" electrode="7" threshold="17" comfort="239"/>
          <channel number="17" electrode="6" threshold="18" comfort="238"/>
          <channel number="18" electrode="5" threshold="19" comfort="237"/>
          <channel number="19" electrode="4" threshold="20" comfort="236"/>
          <channel number="20" electrode="3" threshold="21" comfort="235"/>
          <channel number="21" electrode="2" threshold="22" comfort="234"/>
          <channel number="22" electrode="1" threshold="23" comfort="233"/>
        </inline>
      </defaultmap>
    </device>
  </devices>
  <filters> </filters>
  <stimuli>
    <fixed_parameters/>
    <stimulus id="stimulus1">
      <datablocks>
        <datablock id="rawdata1"/>
      </datablocks>
    </stimulus>
  </stimuli>
  <connections/>
</apex:apex>
