<?xml version='1.0' encoding='UTF-8'?>
<apex:apex xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://med.kuleuven.be/exporl/apex/3.0.3/experiment https://gilbert.med.kuleuven.be/apex/schemas/3.0.3/experiment.xsd"
 xmlns:apex="http://med.kuleuven.be/exporl/apex/3.0.3/experiment"
 version="1">
  
  

  <!-- 
  Regression test for AID (cfr Apex 2)
  1. Sequential
   
    -->

  <procedure xsi:type="apex:constantProcedureType">

    <parameters>
      <presentations>10</presentations>
      <skip>0</skip>
      <order>sequential</order>
    </parameters>

    <trials>
      <trial id="trial1">
        <answer>button1</answer>
        <screen id="screen1"/>
        <stimulus id="stimulus1"/>
      </trial>

    </trials>
  </procedure>

  <corrector xsi:type="apex:isequal"/>
  <!--type>isequal</type-->


  <screens>
    <reinforcement>
      <progressbar>true</progressbar>
      <feedback length="300">true</feedback>
    </reinforcement>
    <screen id="screen1">
      <gridLayout height="2" width="2" id="main_layout">


        <textEdit id="text1" row="1" col="1">
          <font>Times</font>
          <text>times</text>
        </textEdit>


        <textEdit id="text2" row="1" col="2">
          <font>Helvetica</font>
          <text>helvetica</text>
        </textEdit>


      </gridLayout>
      <default_answer_element>text1</default_answer_element>
    </screen>
  </screens>

  <datablocks/>
  <devices/>
  <filters/>

  <stimuli>
    <fixed_parameters/>
    <stimulus id="stimulus1" >
      <description/>
      <datablocks/>
      <variableParameters/>
      <fixedParameters/>
    </stimulus>
  </stimuli>

  <connections/>


</apex:apex>
